{
  "name": "@dreamirl/dreamengine",
  "version": "1.0.0",
  "description": "A simple and fast 2D engine based on PIXI",
  "main": "src/index.js",
  "dependencies": {
    "howler": "^2.0.5",
    "pixi.js": "4.5.6",
    "pixijs-charm": "^0.1.4"
  },
  "devDependencies": {
    "parallel-webpack": "^2.2.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-utf8-bom": "^1.1.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified this is a TODO\" && exit 1",
    "watch": "echo TODO",
    "build": "DEV_BUILD=true FINAL_BUILD=true parallel-webpack --config webpack.config.js --verbose --no-stats",
    "build-dev": "DEV_BUILD=true parallel-webpack --config webpack.config.js --verbose --no-stats",
    "win-build": "set DEV_BUILD=true && set FINAL_BUILD=true&& parallel-webpack --config webpack.config.js --verbose --no-stats",
    "win-build-dev": "set DEV_BUILD=true && parallel-webpack --config webpack.config.js --verbose --no-stats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dreamirl/Dreamengine.git"
  },
  "keywords": [
    "webgl",
    "2D",
    "PIXI",
    "canvas",
    "js",
    "games"
  ],
  "author": "Inateno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dreamirl/Dreamengine/issues"
  },
  "homepage": "https://github.com/Dreamirl/Dreamengine#readme",
  "alias": {
    "PIXI": "pixi.js",
    "P_Charm": "pixijs-charm",
    "stash": "./src/ext_libs/stash",
    "howler-spatial": "node_modules/howler/dist/howler.spatial.min",
    "DE.MainLoop": "./src/MainLoop",
    "DE.about": "./src/about",
    "DE.config": "./src/config",
    "DE.extendPIXI": "./src/utils/extendPIXI",
    "DE.sortGameObjects": "./src/utils/sortGameObjects",
    "DE.Time": "./src/utils/Time",
    "DE.Events": "./src/utils/Events",
    "DE.Achievements": "./src/utils/Achievements",
    "DE.Localization": "./src/utils/Localization",
    "DE.Save": "./src/utils/Save",
    "DE.Audio": "./src/utils/Audio",
    "DE.Notifications": "./src/utils/Notifications",
    "DE.Inputs": "./src/utils/Inputs",
    "DE.gamepad": "./src/utils/gamepad",
    "DE.ImageManager": "./src/utils/ImageManager",
    "DE.Render": "./src/classes/Render",
    "DE.Scene": "./src/classes/Scene",
    "DE.Vector2": "./src/classes/Vector2",
    "DE.Camera": "./src/classes/Camera",
    "DE.BaseRenderer": "./src/classes/renderer/BaseRenderer",
    "DE.TextureRenderer": "./src/classes/renderer/TextureRenderer",
    "DE.SpriteRenderer": "./src/classes/renderer/SpriteRenderer",
    "DE.TilingRenderer": "./src/classes/renderer/TilingRenderer",
    "DE.TextRenderer": "./src/classes/renderer/TextRenderer",
    "DE.GraphicRenderer": "./src/classes/renderer/GraphicRenderer",
    "DE.RectRenderer": "./src/classes/renderer/RectRenderer",
    "DE.GameObject": "./src/classes/GameObject/GameObject",
    "DE.GameObject.automatisms": "./src/classes/GameObject/GameObject.automatisms",
    "DE.GameObject.fade": "./src/classes/GameObject/GameObject.fade",
    "DE.GameObject.focus": "./src/classes/GameObject/GameObject.focus",
    "DE.GameObject.moveTo": "./src/classes/GameObject/GameObject.moveTo",
    "DE.GameObject.scale": "./src/classes/GameObject/GameObject.scale",
    "DE.GameObject.shake": "./src/classes/GameObject/GameObject.shake",
    "DE.GameObject.update": "./src/classes/GameObject/GameObject.update"
  }
}
